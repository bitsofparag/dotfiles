# -*- mode: snippet -*-
# name: ans_setfact
# key: setfact
# --

# https://docs.ansible.com/ansible/latest/modules/set_fact_module.html#examples
- name: ${1:Type}* SET_FACT
  $0${3:when: (project_gitlab_exists.json|length == 1)}
  set_fact:
    cacheable: yes
    ${2:ansible_var}: "{{ $2.json[0] }}"
  tags:
  - $1_debug
