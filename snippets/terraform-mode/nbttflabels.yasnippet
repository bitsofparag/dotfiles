#@module labels
#@description Tf module to add common labels to resources
module "labels" {
  source      = "git::ssh://git@gitlab.com/nbt-standard-platform/infrastructure-as-code.git//modules/aws/labels"
  project     = "$1"
  name        = "$2"
  environment = "$3"
  label_order = ["project", "name", "environment"]
}$0