#@module labels
#@description Tf module to add common labels to resources
module "labels" {
  source      = "app.terraform.io/${1:bitsofparag}/labels/aws"
  project     = var.project_namespace
  name        = var.name
  environment = var.environment
  label_order = ["environment", "project"]
}
